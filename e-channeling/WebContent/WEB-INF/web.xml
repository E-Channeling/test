<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>e-channeling</display-name>
  
  <servlet>
    <servlet-name>AddPatientServlet</servlet-name>
    <servlet-class>com.oop.servlet.AddPatientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginPatientServlet</servlet-name>
    <servlet-class>com.oop.servlet.LoginPatientServlet</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>AddPatientServlet</servlet-name>
    <url-pattern>/AddPatientServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginPatientServlet</servlet-name>
    <url-pattern>/LoginPatientServlet</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>